{
    "$schema": "http://json-schema.org/draft-07/schema#",
    "$id": "https://jsonql.github.io/spec/schema/returnField.schema.json",
    "type": "array",
    "description": "The sequence of property names used to find a specific property.",
    "items": {
        "type": "string"
    },
    "examples": [
        ["name"],
        ["cars", "id"],
        ["cars", "repairs", "price"]
    ],
    "minItems": 1
}